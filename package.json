{
  "name": "isometry",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:native": "cross-env REACT_APP_USE_NATIVE_API=true vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "start:native-api": "node src/server/launch-native-server.js",
    "dev:full-native": "concurrently \"npm run start:native-api\" \"npm run dev:native\"",
    "build:native": "cd native && swift build --target IsometryAPIServer"
  },
  "dependencies": {
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toggle": "^1.1.10",
    "@types/leaflet": "^1.9.21",
    "@types/react-window": "^1.8.8",
    "@uiw/react-md-editor": "^4.0.11",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.8.5",
    "html2pdf.js": "^0.10.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.294.0",
    "mammoth": "^1.7.2",
    "node-pty": "^1.1.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.20.0",
    "react-window": "^1.8.10",
    "sql.js": "^1.9.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3": "^7.4.3",
    "@types/eslint__js": "^8.42.3",
    "@types/html2pdf.js": "^0.10.0",
    "@types/node": "^22.11.7",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.2",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  }
}
